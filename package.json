{
  "name": "markxmind",
  "description": "Create XMind mind maps with Markdown online, export as .xmind/.md/.png/.svg in one click.",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,yml,yaml,html,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "ts-node": "^10.9.2",
    "esbuild": "^0.25.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "typescript-eslint": "^8.29.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  }
}
